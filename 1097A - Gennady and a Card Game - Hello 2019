#include <bits/stdc++.h>
using namespace std;

int main(){
    string s;
    cin>>s;
    bool ans=false;
    for(int i=0;i<5;i++){
        string c;
        cin>>c;
        if(s[0]==c[0] || s[0]==c[1] || s[1]==c[0] || s[1]==c[1]){
            ans=true;
            break;
        }
    }
    if(ans){
        cout<<"YES";
    }
    else{
        cout<<"NO";
    }
    
    
}
