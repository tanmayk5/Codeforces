#include <bits/stdc++.h>
using namespace std;

int main(){
    string s;
    cin>>s;
    int count=1;
    bool ans=false;
    for(int i=1;i<s.size();i++){
        if(s[i]==s[i-1]){
            count++;
        }
        else{
            count=1;
        }
        if(count==7){
            ans=true;
            break;
        }

    }
    if(ans){
        cout<<"YES";
    }
    else
    {
        cout<<"NO";
    }
    
    
    
}
